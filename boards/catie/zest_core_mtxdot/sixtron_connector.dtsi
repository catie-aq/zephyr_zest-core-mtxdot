                   <PWM2 0 &GPIO_PORT PIN_NUMBER 0>,
                   <PWM3 0 &GPIO_PORT PIN_NUMBER 0>,
                   <ICAPT 0 &GPIO_PORT PIN_NUMBER 0>,
                   <WKUP 0 &GPIO_PORT PIN_NUMBER 0>,
                   <DIO1 0 &GPIO_PORT PIN_NUMBER 0>,
                   <DIO2 0 &GPIO_PORT PIN_NUMBER 0>,
                   <DIO3 0 &GPIO_PORT PIN_NUMBER 0>,
                   <DIO4 0 &GPIO_PORT PIN_NUMBER 0>,
                   <ADC1 0 &GPIO_PORT PIN_NUMBER 0>,
