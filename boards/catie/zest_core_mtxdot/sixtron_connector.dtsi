                   <CAN_TX 0 &GPIO_PORT PIN_NUMBER 0>,
                   <I2C_SCL 0 &GPIO_PORT PIN_NUMBER 0>,
                   <I2C_SDA 0 &GPIO_PORT PIN_NUMBER 0>,
                   <UART_RX 0 &GPIO_PORT PIN_NUMBER 0>,
